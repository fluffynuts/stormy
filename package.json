{
  "name": "weather",
  "version": "1.0.0",
  "description": "weather app",
  "main": "./app/main.js",
  "scripts": {
    "lint": "tslint src/**/*.ts",
    "clean": "rimraf app/*",
    "prebuild": "run-p clean lint",
    "build": "webpack --config webpack.config.js",
    "start": "electron .",
    "build-and-run": "run-s build start",
    "watch": "nodemon -w src -e ts,html,css,js -x \"npm run build-and-run\"",
    "prepackage": "run-s build",
    "package": "electron-packager . --out packages --platform=win32 --overwrite",
    "dev": "cross-env TARGET=web webpack-dev-server --debug --devtool #inline-source-map"
  },
  "author": "Davyd McColl <davydm@gmail.com> (https://github.com/fluffynuts)",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.6.1",
    "angular-animate": "^1.6.1",
    "angular-aria": "^1.6.1",
    "angular-material": "^1.1.1",
    "angular-storage": "0.0.15",
    "angular-ui-router": "^0.3.2",
    "electron-debug": "^1.1.0",
    "electron-prebuilt": "^1.4.13",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1"
  },
  "devDependencies": {
    "@types/angular": "^1.6.1",
    "@types/angular-material": "^1.1.44",
    "@types/angular-ui-router": "^1.1.36",
    "@types/electron": "^1.4.31",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "cross-env": "^3.1.4",
    "devtron": "^1.4.0",
    "electron-packager": "^8.5.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.26.0",
    "json-loader": "^0.5.4",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.0",
    "rimraf": "^2.5.4",
    "script-loader": "^0.7.0",
    "tslint": "^4.3.1",
    "typescript": "^2.1.5",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-target-electron-renderer": "^0.4.0"
  }
}
